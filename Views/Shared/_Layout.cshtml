<!DOCTYPE html>
<html lang="en">
<head 4f6230eed0>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - mvc_first_proj</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link href="css/Layout.css" rel="stylesheet">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Lato:300,400,700,900&display=swap" rel="stylesheet">
    <!-- Bootstrap CSS -->
    <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.min.css'>
    <!-- Font Awesome CSS -->
    <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.1/css/all.min.css'>
   @* <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/mvc_first_proj.styles.css" asp-append-version="true" />*@

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-light" style="background-color:#A74AC7;">
            <div class="container-fluid" style="background-color:#A74AC7;">
                <ul class="navbar-nav pe-5 me-5">

                    <li class="navbar-brand">
                       
                            <img src="https://inkbotdesign.com/wp-content/uploads/2023/02/NYU-logo-design.png?ezimgfmt=ng:webp/ngcb15" alt="" width="70" height="50" class=" shadow-lg">
                             
                       
                    </li>
                </ul>


               
               
               
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav pe-5 me-auto mb-2 mt-2 ">

                        <li class="nav-item">
                           <a class="fa-solid fa-house nav-link pe-2 colours"  href="#">  Home</a>
                        </li>

                         <li class="nav-item">
                            <a class="fa-solid fa-user nav-link pe-2" href="#">  Profile</a>
                        </li>

                        <li class="nav-item">
                            <a class="fa-solid fa-wallet nav-link pe-2" href="#">  Billing</a>
                        </li>

                        <li class="nav-item">
                            <a class="fa-solid fa-chart-pie nav-link pe-2" href="#">  Analytics</a>
                        </li>

                        <li class="nav-item">
                            <a class="fa-solid fa-list-check nav-link pe-3" href="#">  Task</a>
                        </li>

                        <li class="nav-item">
                            <a class="fa-solid fa-right-from-bracket nav-link pe-3" href="#">logout</a>
                            
                        </li>
 
                       
                       
                       
                    </ul>
                    <form class="d-flex">
                        <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                        <button class="btn btn-outline-dark" type="submit">Search</button>
                    </form>
                </div>
            </div>
        </nav>























    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - mvc_first_proj - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="https://kit.fontawesome.com/4f6230eed0.js" crossorigin="anonymous"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
